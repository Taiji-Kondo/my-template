{
  "name": "my_template",
  "version": "1.0.0",
  "scripts": {
    "gulp-ejs": "gulp ejs",
    "gulp-watch": "gulp watch",
    "gulp-clean": "gulp clean",
    "gulp-php": "gulp php",
    "gulp-initial-file": "gulp initialFile",
    "@start": "run-p gulp-watch webpack-watch @imagemin gulp-php server",
    "@start-php": "run-p webpack-watch @imagemin gulp-php server",
    "@start-wp": "run-p gulp-watch webpack-watch @imagemin gulp-initial-file server",
    "start": "NODE_ENV=development run-s gulp-ejs @start",
    "start-pro": "NODE_ENV=production run-s gulp-ejs @start",
    "start-php": "NODE_ENV=development run-s @start-php",
    "start-php-pro": "NODE_ENV=production run-s @start-php",
    "start-wp": "NODE_ENV=development run-s @start-wp",
    "start-wp-pro": "NODE_ENV=production run-s @start-wp",
    "build": "NODE_ENV=production run-s gulp-clean gulp-ejs imagemin webpack-build",
    "build-php": "NODE_ENV=production run-s gulp-clean gulp-initial-file",
    "server": "browser-sync start --config @config/bs-config.js",
    "@imagemin": "run-p imagemin watch:imagemin",
    "imagemin": "node @config/imagemin.js",
    "watch:imagemin": "onchange 'src/img/' -e '**/*.DS_Store' -- npm run imagemin",
    "webpack-build": "webpack",
    "webpack-watch": "webpack -w"
  },
  "dependencies": {
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/smooth-scroll": "^16.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.0.2",
    "browser-sync": "^2.26.13",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "del": "^6.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^2.3.0",
    "extract-css-chunks-webpack-plugin": "^4.8.0",
    "fibers": "^5.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gsap": "^3.5.1",
    "gulp": "^4.0.2",
    "gulp-beautify": "^3.0.0",
    "gulp-connect-php": "^1.0.3",
    "gulp-ejs": "^5.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "import-glob-loader": "^1.1.0",
    "lodash": "^4.17.20",
    "nodelist-foreach-polyfill": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "picturefill": "^3.0.3",
    "postcss": "^8.1.8",
    "postcss-loader": "^4.1.0",
    "postcss-object-fit-images": "^1.1.2",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tailwindcss": "^1.8.9",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-fix-style-only-entries": "^0.6.0",
    "webpack-serve": "^3.2.0"
  }
}
